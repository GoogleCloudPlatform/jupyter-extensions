{
  "private": true,
  "name": "jupyterlab_gcpscheduler",
  "version": "1.0.2",
  "description": "GCP Notebooks Scheduler Extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "Apache",
  "author": "GCP AI Platform Notebooks Frontend Team",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/gcpsettings.json",
    "style/*.css",
    "style/images/*.png"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleCloudPlatform/jupyter-extensions"
  },
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "build-python": "npm pack && python setup.py sdist bdist_wheel",
    "clean": "rimraf build dist lib *tsbuildinfo *tgz",
    "install-extension": "../dev-install.sh",
    "lint-warnings": "eslint \"src/**/*.{ts,tsx}\" --fix --quiet",
    "lint": "npm run lint-warnings -- --quiet",
    "pretest": "npm run lint",
    "prepack": "npm run clean && npm run build",
    "test": "jest --coverage",
    "test-watch": "jest --watch",
    "watch": "tsc -b -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.2.1",
    "@jupyterlab/apputils": "^1.2.1",
    "@jupyterlab/coreutils": "^3.2.0",
    "@jupyterlab/docregistry": "^1.2.1",
    "@jupyterlab/notebook": "^1.2.2",
    "@jupyterlab/services": "^4.2.2",
    "@material-ui/core": "^3.9.4",
    "@material-ui/icons": "^3.0.2",
    "@phosphor/algorithm": "^1.2.0",
    "@phosphor/disposable": "^1.3.1",
    "@phosphor/signaling": "^1.3.0",
    "csstips": "^1.2.0",
    "formik": "^1.5.8",
    "gcp-jupyterlab-shared": "^1.0.0",
    "typestyle": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@jupyterlab/testutils": "^1.2.2",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/fs-extra": "^8.1.0",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.51",
    "@types/gapi.client.cloudfunctions": "^1.0.1",
    "@types/gapi.client.ml": "^1.0.1",
    "@types/gapi.client.servicemanagement": "^1.0.2",
    "@types/gapi.client.storage": "^1.0.1",
    "@types/jest": "^24.9.1",
    "@types/react": "~16.8.18",
    "@types/react-dom": "^16.8.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "~16.8.4",
    "react-dom": "~16.8.4"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema",
    "discovery": {
      "server": {
        "base": {
          "name": "jupyterlab_gcpscheduler",
          "managers": [
            "pip"
          ]
        }
      }
    }
  }
}
