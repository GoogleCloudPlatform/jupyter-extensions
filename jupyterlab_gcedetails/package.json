{
  "name": "jupyterlab_gcedetails",
  "version": "0.1.0",
  "description": "Google Compute Engine Instance Details extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "Apache",
  "author": "GCP AI Platform Notebooks Frontend Team",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/*.css",
    "style/images/*.png"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "clean": "rimraf lib *tsbuildinfo",
    "e2e-test": "jest -c jest-puppeteer.config.js",
    "prepack": "npm run clean && npm run build",
    "lint-warnings": "eslint \"src/**/*.{ts,tsx}\" --fix --quiet",
    "lint": "npm run lint-warnings -- --quiet",
    "start": "concurrently -k -p \"[{name}]\" -n \"Typescript,JupyterLab\" -c \"green.bold,yellow.bold\" \"npm run watch\" \"jupyter lab --watch\"",
    "devserver": "nodemon -w jupyterlab_gcedetails -e \".py\" -x \"jupyter lab --no-browser\"",
    "test": "jest --coverage",
    "test-watch": "jest --watch",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.2.1",
    "@jupyterlab/apputils": "^1.2.1",
    "@jupyterlab/coreutils": "^3.2.0",
    "@jupyterlab/docregistry": "^1.2.1",
    "@jupyterlab/filebrowser": "^1.2.1",
    "@jupyterlab/mainmenu": "^1.2.1",
    "@jupyterlab/notebook": "^1.2.2",
    "@jupyterlab/statusbar": "^1.2.1",
    "@material-ui/core": "^3.9.4",
    "@material-ui/icons": "^3.0.2",
    "@phosphor/disposable": "^1.3.1",
    "@phosphor/widgets": "^1.9.3",
    "csstips": "^1.2.0",
    "gcp-jupyterlab-shared": "^1.0.0",
    "typestyle": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@jupyterlab/testutils": "^1.2.2",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/expect-puppeteer": "^4.4.1",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^24.9.1",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/puppeteer": "^2.0.1",
    "@types/react": "~16.8.18",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "concurrently": "^5.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "jest-puppeteer": "^4.4.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "puppeteer": "^3.0.2",
    "rimraf": "^3.0.0",
    "simulate-event": "^1.4.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "~16.8.4",
    "react-dom": "~16.8.4"
  },
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "base": {
          "name": "jupyterlab_gcedetails",
          "managers": [
            "pip"
          ]
        }
      }
    }
  }
}
