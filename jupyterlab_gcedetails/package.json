{
  "private": true,
  "name": "jupyterlab_gcedetails",
  "version": "0.1.0",
  "description": "Google Compute Engine Instance Details extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "Apache",
  "author": "GCP AI Platform Notebooks Frontend Team",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/*.css",
    "style/images/*.png"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "build-python": "npm pack && python setup.py sdist bdist_wheel",
    "clean": "rimraf build dist lib *tsbuildinfo *tgz",
    "e2e-test": "jest -c jest-puppeteer.config.js",
    "install-extension": "../scripts/dev-install.sh",
    "lint-warnings": "eslint \"src/**/*.{ts,tsx}\" --fix --quiet",
    "lint": "npm run lint-warnings -- --quiet",
    "pretest": "npm run lint",
    "prepack": "npm run clean && npm run build",
    "test": "jest --coverage",
    "test-python": "../scripts/run_python_tests.sh coverage",
    "test-watch": "jest --watch",
    "watch": "tsc -b -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.2.1",
    "@jupyterlab/apputils": "^1.2.1",
    "@jupyterlab/services": "^4.2.2",
    "@jupyterlab/statusbar": "^1.2.1",
    "csstips": "^1.2.0",
    "gcp_jupyterlab_shared": "^1.0.5",
    "recharts": "^1.8.5",
    "typestyle": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/expect-puppeteer": "^4.4.1",
    "@types/jest": "^24.9.1",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/puppeteer": "^2.0.1",
    "@types/react": "~16.8.18",
    "@types/react-dom": "^16.8.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "expect-puppeteer": "^4.4.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^3.0.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "~16.8.4",
    "react-dom": "~16.8.4"
  },
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "base": {
          "name": "jupyterlab_gcedetails",
          "managers": [
            "pip"
          ]
        }
      }
    }
  }
}
